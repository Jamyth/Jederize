{
	"name": "jederize",
	"version": "1.0.0",
	"main": "dist/index.js",
	"license": "MIT",
	"scripts": {
		"run-script": "ts-node --project config/tsconfig.script.json",
		"format": "yarn run-script script/format.ts",
		"lint": "yarn run-script script/lint.ts",
		"build": "yarn run-script script/build.ts",
		"test": "mocha --require ts-node/register ./test/**/*.test.ts",
		"watch": "tsc -watch -p config/tsconfig.src.json"
	},
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"commands": [
			{
				"command": "jederize.folderize",
				"title": "Jederize"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "jederize.folderize",
					"group": "MyGroup@1",
					"when": "resourceScheme == file"
				}
			],
			"commandPalette": [
				{
					"command": "jederize.folderize",
					"when": "false"
				}
			]
		}
	},
	"files": [
		"dist/**"
	],
	"devDependencies": {
		"@iamyth/logger": "1.0.4",
		"@iamyth/prettier-config": "0.0.1",
		"@types/node": "15.6.1",
		"@types/vscode": "1.56.0",
		"eslint-config-iamyth": "0.0.6",
		"mocha": "8.4.0",
		"prettier": "2.3.0",
		"ts-node": "10.0.0",
		"typescript": "4.3.2",
		"vsce": "1.91.0"
	}
}
